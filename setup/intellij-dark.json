{
  "name": "IntelliJ Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#1E1F22",
    "editor.foreground": "#BCBEC4",
    "editorLineNumber.foreground": "#4B5059",
    "editorCursor.foreground": "#CED0D6",
    "editor.selectionBackground": "#2B4C6F",
    "editor.lineHighlightBackground": "#26282E"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#7A7E85",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#CF8E6D"
      }
    },
    {
      "scope": ["keyword.control", "keyword.operator.new"],
      "settings": {
        "foreground": "#CF8E6D"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#6AAB73"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language.numeric"],
      "settings": {
        "foreground": "#2AACB8"
      }
    },
    {
      "scope": ["constant.language.boolean", "constant.language.null"],
      "settings": {
        "foreground": "#CF8E6D"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#56A8F5"
      }
    },
    {
      "scope": ["meta.function-call.generic", "meta.method-call"],
      "settings": {
        "foreground": "#56A8F5"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": {
        "foreground": "#BCBEC4"
      }
    },
    {
      "scope": ["variable.parameter", "meta.parameter"],
      "settings": {
        "foreground": "#BCBEC4"
      }
    },
    {
      "scope": ["variable.other.property", "variable.other.object.property", "support.variable.property"],
      "settings": {
        "foreground": "#C77DBB"
      }
    },
    {
      "scope": ["variable.other.constant", "variable.other.enummember"],
      "settings": {
        "foreground": "#C77DBB",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.type.parameter", "storage.type.generic"],
      "settings": {
        "foreground": "#16BAAC"
      }
    },
    {
      "scope": ["meta.annotation", "storage.type.annotation", "punctuation.definition.annotation"],
      "settings": {
        "foreground": "#B3AE60"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#CF8E6D"
      }
    },
    {
      "scope": ["punctuation", "punctuation.separator", "punctuation.terminator"],
      "settings": {
        "foreground": "#BCBEC4"
      }
    },
    {
      "scope": ["punctuation.section.embedded"],
      "settings": {
        "foreground": "#BCBEC4"
      }
    },
    {
      "scope": ["variable.language.this", "variable.language.super"],
      "settings": {
        "foreground": "#CF8E6D"
      }
    },
    {
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#BCBEC4"
      }
    },
    {
      "scope": ["variable.other.readwrite", "variable.other.object"],
      "settings": {
        "foreground": "#BCBEC4"
      }
    },
    {
      "scope": ["support.constant"],
      "settings": {
        "foreground": "#C77DBB"
      }
    },
    {
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#CF8E6D"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#BCBEC4"
      }
    },
    {
      "scope": ["storage.type.function.arrow"],
      "settings": {
        "foreground": "#BCBEC4"
      }
    },
    {
      "scope": ["meta.brace", "punctuation.section"],
      "settings": {
        "foreground": "#BCBEC4"
      }
    },
    {
      "scope": ["entity.name.label"],
      "settings": {
        "foreground": "#32B8AF"
      }
    },
    {
      "scope": ["variable.parameter.function.language.special"],
      "settings": {
        "foreground": "#56C1D6"
      }
    }
  ]
}
